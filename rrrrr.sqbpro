CREATE TABLE IF NOT EXISTS pedido_item (
    id_pedido_item INTEGER PRIMARY KEY,
    id_pedido INTEGER NOT NULL,
    id_producto INTEGER NOT NULL,
    cantidad INTEGER NOT NULL, 
    precio_unitario INTEGER NOT NULL,
